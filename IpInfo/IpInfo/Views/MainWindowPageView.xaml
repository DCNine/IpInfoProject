<Window x:Class="IpInfo.Views.MainWindowPageView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IpInfo.ViewModels"
        mc:Ignorable="d"
        Title="MainWindowPageView" Height="300" Width="300">
    <Window.DataContext>
        <local:MainWindowPageViewModel/>
    </Window.DataContext>
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="136,113,0,0" VerticalAlignment="Top" Width="75" Command="{Binding DoTestCommand}"/>

    </Grid>
</Window>
